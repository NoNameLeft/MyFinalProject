@model BLTC.Web.ViewModels.Orders.ShippingInfoInputModel

<div id="additionalData">
    <h3>Shipping Address</h3>

    <label asp-for="ShippingAddress"> <i class="fa fa-address-card-o"></i> Address</label>
    <input type="text" asp-for="ShippingAddress">

    <label asp-for="ShippingTown"> <i class="fa fa-home" style="font-size:20px"></i> City</label>
    <input type="text" asp-for="ShippingTown">

    <label asp-for="ShippingState"> <i class="fa fa-institution"></i> State</label>
    <input type="text" asp-for="ShippingState">

    <label asp-for="ShippingZipCode"> <i class="fa fa-map-marker" style="font-size:20px"></i> Zip</label>
    <input type="text" asp-for="ShippingZipCode">

</div>
